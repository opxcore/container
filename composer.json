{
    "name": "opxcore/container",
    "description": "The OpxCore dependency injection container.",
    "license": "MIT",
    "homepage": "https://opxcore.com",
    "support": {
        "source": "https://github.com/opxcore/container",
        "issues": "https://github.com/opxcore/container/issues"
    },
    "authors": [
        {
            "name": "Lozovoy Vyacheslav",
            "email": "opxcore@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "opxcore/container-interface": "^1.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.0"
    },
    "autoload": {
        "psr-4": {
            "OpxCore\\Container\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpxCore\\Tests\\Container\\": "tests/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
